{
  "manifest_version": 3,
  "name": "LeetCode Stats Fetcher",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  "permissions": [
    "storage"
  ]
}
