{
    "manifest_version": 3,
    "name": "LeetCode User Stats",
    "version": "1.0",
    "description": "Shows LeetCode stats for a user",
    "icons": {
      "48": "icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab",
      "storage",
  "https://leetcode.com/*"
    ]
  }
  